	margin-top: 30px;
	height: 150px;
	
}
.content_center .bottom_menu_img div{
	height: 150px;
	width:87px;
	float: left;
}

.content_right .panel-heading{
	position: relative;
}
.content_right .panel-heading span{
	font-size: 14px;
}
.content_right .panel-heading .esc{
	display: inline-block;
	position: absolute;
	right: 15px;
	text-align: center;
	width: 40px;
	height: 20px;
	border-radius: 5px;
	background-color: lightgray;
	color: aquamarine;	
}
.content_right .panel-heading #userName{
	color: red;
}
.content_right .panel-body{
	height: 360px;
}
.content_right .panel-body table td .add_lesson{
	text-align: center;
	width: 40px;
	height: 20px;
	border-radius: 5px;
}
.content_right .panel-body table td>span{
	color: #1C1C1C;
	display: inline-block;
	width: 60px;
	/*border: 1px solid aqua;*/
}
.content_right .panel-body table td>a{
	/*float: right;*/
	display: inline-block;
	color: #F4A460;
	/*border: 1px solid aqua;*/
}
.content_right .panel-body table td .add_lesson{
	color: #00BFFF;
}
.content_center .item .carousel-caption{
	font-family: "微软雅黑";
	font-size: 16px;
	color: aqua;
}
.content_right .panel{
	margin: 0px -15px;
}
.content1 .pop_win{
	width: 450px;
	position: absolute;
	left: 0px;
	background-color: #FDF5E6;
	border-radius: 5px;
	border: 2px solid #F0F8FF;
	opacity: 0.9;
	z-index: 999999999;
}
.content1 .pop_win table{
	border-bottom: 1px dashed #EEE5DE;
}
.content1 .pop_win table td,.content1 .pop_win table th{
	/*width: 125px;*/
	border: none;
}
.content1 .pop_win table td{
	height: 20px;
	/*border: 1px solid aqua;*/
	width: 120px;
}
.content1 .pop_win table td a{
	font-size: 12px;
	color: black;
	/*width: 125px;*/
}
.content1 .pop_win table th{
	color: black;
	width: 150px;
	/*border: 1px solid black;*/
}
