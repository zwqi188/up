	background-color: #FFEBCD;
	width: 110px;
}
.sch_div .wrap_sch .sch_txt{
	width: 250px;
}
.sch_div .wrap_sch .sch_btn{
	width: 100px;
	background-color: #FE9813;
	color: white;
}

.content1{
	/*border: 1px solid aqua;*/
	position: relative;
	/*height: 400px;*/
}
.content_left,.content_center,.content_right{
	margin: 0px;
	/*border: 1px solid aqua;*/
